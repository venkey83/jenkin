<?xml version="1.0" encoding="utf-8"?>
<databaseChangeLog
    xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.4.xsd">
    <!--
        Added the constraints for entity BuyerSupplierMapping.
    -->
    <changeSet id="20161219125041-2" author="jhipster">
        
        <addForeignKeyConstraint baseColumnNames="buyer_id"
                                 baseTableName="buyer_supplier_mapping"
                                 constraintName="fk_buyer_supplier_mapping_buyer_id"
