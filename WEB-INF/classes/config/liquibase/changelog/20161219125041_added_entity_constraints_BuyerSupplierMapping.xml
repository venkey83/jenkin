                                 baseTableName="buyer_supplier_mapping"
                                 constraintName="fk_buyer_supplier_mapping_supplier_id"
                                 referencedColumnNames="id"
                                 referencedTableName="supplier"/>

    </changeSet>
</databaseChangeLog>
