<?xml version="1.0" encoding="utf-8"?>
<databaseChangeLog
    xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.4.xsd">
    <!--
        Added the constraints for entity Supplier.
    -->
    <changeSet id="20161212110650-2" author="jhipster">

        <addForeignKeyConstraint baseColumnNames="created_user_id"
                                 baseTableName="supplier"
                                 constraintName="fk_supplier_created_by_id"
                                 referencedColumnNames="id"
                                 referencedTableName="user"/>

    </changeSet>
    <changeSet id="20181031180300-3" author="jhipster">
        <addDefaultValue tableName="supplier" columnName="ezypayment_only" defaultValueComputed="false"/>
    </changeSet>
</databaseChangeLog>
