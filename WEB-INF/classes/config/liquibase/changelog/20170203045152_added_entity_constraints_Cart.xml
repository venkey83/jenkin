<?xml version="1.0" encoding="utf-8"?>
<databaseChangeLog
    xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.4.xsd">
    <!--
        Added the constraints for entity Cart.
    -->
    <changeSet id="20170203045152-2" author="jhipster">

        <addForeignKeyConstraint baseColumnNames="buyer_user_id"
                                 baseTableName="cart"
                                 constraintName="fk_cart_buyer_user_id"
                                 referencedColumnNames="id"
                                 referencedTableName="buyer_user"/>

        <addForeignKeyConstraint baseColumnNames="items_id"
                                 baseTableName="cart"
                                 constraintName="fk_cart_items_id"
                                 referencedColumnNames="id"
                                 referencedTableName="items"/>

    </changeSet>
    <changeSet id="20170203045152-3" author="jhipster">

    <addForeignKeyConstraint baseColumnNames="buyer_business_id"
                             baseTableName="cart"
                             constraintName="fk_cart_buyer_business_id"
                             referencedColumnNames="id"
                             referencedTableName="buyer_business"/>
    </changeSet>
    <changeSet id="20170203045152-4" author="jhipster">

        <addForeignKeyConstraint baseColumnNames="gl_code_mappings_id"
                                 baseTableName="cart"
                                 constraintName="fk_cart_gl_code_mappings_id"
                                 referencedColumnNames="id"
                                 referencedTableName="gl_code_mappings"/>
    </changeSet>

</databaseChangeLog>
